VPATH = ../recore
CC = gcc
CXX = g++
CFLAGS = -Wall -g -O2
CXXFLAGS = $(CFLAGS)

OBJECTS = \
	lib_cec6par.o \
	lib_financial.o \
	lib_geothermal.o \
	lib_irradproc.o \
	lib_physics.o \
	lib_powerblock.o \
	lib_pvinv.o \
	lib_pvmodel.o \
	lib_pvshade.o \
	lib_pvwatts.o \
	lib_sandia.o \
	lib_util.o \
	lib_weatherfile.o \
	lib_windfile.o \
	lib_windwatts.o


TARGET=recore.a

$(TARGET):$(OBJECTS)
	ar rs $(TARGET) $(OBJECTS)

clean:
	rm -rf $(TARGET) $(OBJECTS)
